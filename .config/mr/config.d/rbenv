[$XDG_CONFIG_HOME/vcsh/repo.d/rbenv.git]
checkout = vcsh clone git@github.com:marklee77/vcsh-homedir-rbenv.git rbenv
commit   = vcsh common commit -a
update   = vcsh common pull
push     = vcsh common push
status   = vcsh common status
gc       = vcsh common gc

[.rbenv]
checkout = git clone https://github.com/sstephenson/rbenv.git .rbenv
push = /bin/true

[.rbenv/plugins/ruby-build]
checkout = git clone https://github.com/sstephenson/ruby-build.git ruby-build
push = /bin/true
