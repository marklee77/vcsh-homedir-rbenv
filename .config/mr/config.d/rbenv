[${XDG_CONFIG_HOME:-$HOME/.config}/vcsh/repo.d/rbenv.git]
checkout = vcsh clone https://github.com/marklee77/vcsh-homedir-rbenv.git rbenv
commit   = vcsh rbenv commit -a
update   = vcsh rbenv pull
push     = vcsh rbenv push git@github.com:marklee77/vcsh-homedir-rbenv.git 
status   = vcsh rbenv status
gc       = vcsh rbenv gc

[.rbenv]
checkout = git clone https://github.com/sstephenson/rbenv.git .rbenv
push = /bin/true

[.rbenv/plugins/ruby-build]
checkout = git clone https://github.com/sstephenson/ruby-build.git ruby-build
push = /bin/true
